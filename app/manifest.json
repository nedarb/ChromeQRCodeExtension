{
  "name": "__MSG_appName__",
  "version": "0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/qr-16.png",
    "128": "images/qr-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "background",
    "management",
    "tabs"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": {
      "19": "images/qr-19.png",
      "38": "images/qr-38.png"
    },
    "default_title": "__MSG_appName__",
    "default_popup": "popup.html"
  }
}
